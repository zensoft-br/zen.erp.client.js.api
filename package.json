{
  "name": "@zensoftbr/zenerpclient",
  "version": "1.0.21",
  "description": "Javascript client for Zen ERP",
  "type": "module",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": {
      "require": "./dist/cjs/*.js",
      "import": "./dist/esm/*.js",
      "default": "./dist/esm/*.js"
    }
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/zensoft-br/zen.client.javascript.git"
  },
  "bugs": "",
  "keywords": [
    "Zen ERP",
    "Zen Soft"
  ],
  "author": {
    "name": "Fabiano Bonin",
    "email": "fabiano.bonin@zensoft.com.br",
    "url": ""
  },
  "files": [
    "dist/",
    "LICENSE.txt",
    "README.md"
  ],
  "contributors": [],
  "scripts": {
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:types": "tsc -p tsconfig.types.json",
    "build": "npm run build:types && npm run build:cjs && npm run build:esm",
    "prepublishOnly": "npm run build",
    "bundle": "rollup --config",
    "test": "echo \"Error: no test specified\" && exit 1",
    "zip": "npx npm-pack-zip"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@stylistic/eslint-plugin-js": "^1.6.2",
    "@types/lodash.debounce": "^4.0.9",
    "@types/luxon": "^3.4.2",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "rollup": "^4.13.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "lodash.debounce": "^4.0.8",
    "luxon": "^3.4.4"
  }
}