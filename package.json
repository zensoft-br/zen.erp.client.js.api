{
  "name": "@zensoftbr/zenerpclient",
  "version": "1.0.38",
  "description": "Zen ERP Client Api",
  "type": "module",
  "types": "./dist/types/index.d.ts",
  "license": "ISC",
  "private": false,
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": {
      "require": "./dist/cjs/*.js",
      "import": "./dist/esm/*.js"
    }
  },
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 6.0.0"
  },
  "files": [
    "dist/",
    "LICENSE.txt",
    "README.md"
  ],
  "homepage": "https://github.com/zensoft-br/zen.erp.client.js.api",
  "repository": {
    "type": "git",
    "url": "https://github.com/zensoft-br/zen.erp.client.js.api.git"
  },
  "bugs": {
    "url": "https://github.com/zensoft-br/zen.erp.client.js.api/issues"
  },
  "keywords": [
    "Zen ERP",
    "JavaScript",
    "client"
  ],
  "author": {
    "name": "Zen Soft",
    "email": "contact@zensoft.com.br",
    "url": "https://www.zensoft.com.br"
  },
  "contributors": [],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:types": "tsc -p tsconfig.types.json",
    "dev": "echo 'No dev script defined'",
    "pack": "npm pack",
    "prepublishOnly": "npm run build",
    "release": "npm publish",
    "test": "vitest"
  },
  "dependencies": {
    "luxon": "^3.4.4"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin-js": "^1.6.2",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.15.21",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "tslib": "^2.6.2",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  }
}